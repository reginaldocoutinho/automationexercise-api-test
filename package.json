  },
  "homepage": "https://github.com/reginaldocoutinho/automationexercise-api-test#readme",
  "devDependencies": {
    "cypress": "^15.9.0",
    "merge-json-file": "^1.0.1",
    "mochawesome": "^7.1.4",
    "mochawesome-merge": "^5.1.0",
    "mochawesome-report-generator": "^6.3.2",
    "rimraf": "^6.1.2"
  }
}
